            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>com.denodo.vdp.jdbc</groupId>
            <artifactId>denodo-jdbc</artifactId>
            <version>${denodo-jdbc.version}</version>
        </dependency>
        <dependency>
            <groupId>com.denodo.vdp.jdbc</groupId>
            <artifactId>denodo-hibernate-dialect</artifactId>
            <version>${denodo-hibernate-dialect.version}</version>
        </dependency>

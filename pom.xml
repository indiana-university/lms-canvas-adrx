    <properties>
        <!-- The denodo-jdbc driver version 7.0.5 maps to the platform version 7.0 20190903 -->
        <denodo-jdbc.version>7.0.5</denodo-jdbc.version>
        <denodo-hibernate-dialect.version>7.0.20190403</denodo-hibernate-dialect.version>
        <lms-email-service.version>5.0.0-SNAPSHOT</lms-email-service.version>
        <lms-canvas-common-configuration.version>5.0.2-SNAPSHOT</lms-canvas-common-configuration.version>
